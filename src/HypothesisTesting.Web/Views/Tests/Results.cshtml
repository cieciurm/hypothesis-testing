@model TestResultViewModel

@if (Model.HasError)
{
    <div class="alert alert-danger">
        @Model.GetTranslation(WebConstants.Translations.Error)
    </div>
}

<div class="row">
    <div class="col-12">
        <ul>
            @for (var i = 0; i < Model.Logs.Count; i++)
            {
                if (i == Model.Logs.Count - 1)
                {
                    <li><b>@Model.Logs[i]</b></li>
                }
                else
                {
                    <li>@Model.Logs[i]</li>
                }
            }
            @if (!string.IsNullOrWhiteSpace(Model.PValue))
            {
                <li><b>p-value: @Model.PValue</b></li>
            }
        </ul>
    </div>
</div>

